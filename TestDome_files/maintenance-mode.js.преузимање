TESTDOME=TESTDOME||{},TESTDOME.maintenanceModeBundleLoaded=!0,$(document).ready(function(){var t="RT_MaintenanceInfo",e=TESTDOME.adminUI.getCookie(t);e&&(JSON.parse(e).HasDismissedAlert||$("#maintenance-message-container").show());$(document).on("close.bs.alert","#maintenance-message-alert",function(){return $("#maintenance-message-alert-dialog").modal("show"),!1}),$(document).on("click","#btn-maintenance-message-alert-dialog",function(){var e=TESTDOME.adminUI.getCookie(t),n=JSON.parse(e);n.HasDismissedAlert=!0;var a=new Date;a.setHours(a.getHours()+8),TESTDOME.adminUI.setCookie(t,JSON.stringify(n),a),$("#maintenance-message-container").hide()})});
